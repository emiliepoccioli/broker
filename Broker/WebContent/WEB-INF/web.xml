<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Broker</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>BrokerList</servlet-name>
    <servlet-class>com.dell.storagebroker.servlets.BrokerList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BrokerUpload</servlet-name>
    <servlet-class>com.dell.storagebroker.servlets.BrokerUpload</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BrokerUploadPolicies</servlet-name>
    <servlet-class>com.dell.storagebroker.servlets.BrokerUploadPolicies</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BrokerList</servlet-name>
    <url-pattern>/BrokerList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BrokerUpload</servlet-name>
    <url-pattern>/BrokerUpload</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BrokerUploadPolicies</servlet-name>
    <url-pattern>/BrokerUploadPolicies</url-pattern>
  </servlet-mapping>
</web-app>